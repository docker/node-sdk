{
    "$schema": "./node_modules/oxlint/configuration_schema.json",
    "plugins": ["import", "typescript", "unicorn"],
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {},
    "settings": {},
    "rules": {
        "eqeqeq": "error",
        "import/no-cycle": "error",
        "no-unused-vars": [
            "error",
            {
                "caughtErrors": "none",
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_",
                "destructuredArrayIgnorePattern": "^_"
            }
        ]
    },
    "overrides": []
}
